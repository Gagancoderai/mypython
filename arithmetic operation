num1= int(input("enter your num1:"))
num2= float(input("enter your num2:"))
print("num1 is:",num1)
print("num2 is:",num2)
addition= num1+num2
sub= num1-num2
div1 = num1/num2
div2 = num1//num2
div3 = num1%num2
mul = num1*num2
print(addition,sub,div1,div2,div3,mul)
